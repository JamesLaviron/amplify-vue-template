{
  "name": "amplify-vue-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-amplify/ui-vue": "^4.3.3",
    "@aws-sdk/client-dynamodb": "^3.859.0",
    "@aws-sdk/client-ses": "^3.859.0",
    "@aws-sdk/credential-providers": "^3.859.0",
    "@tailwindcss/vite": "^4.1.11",
    "@vueuse/core": "^13.6.0",
    "aws-amplify": "^6.6.6",
    "lucide-react": "^0.536.0",
    "node-fetch": "^3.3.2",
    "radix-vue": "^1.9.17",
    "shadcn-vue": "^2.2.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.5.0",
    "@aws-amplify/backend-cli": "^1.2.9",
    "@aws-amplify/seed": "^1.0.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@nuxt/ui": "^3.3.0",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.12.5",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "aws-cdk": "^2.137.0",
    "aws-cdk-lib": "^2.137.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "constructs": "^10.3.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.4",
    "lucide-vue-next": "^0.536.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.7.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.4.5",
    "vite": "^5.4.10",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.0.11"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css,scss,yaml,yml}": [
      "prettier --write"
    ]
  }
}
